<h1>Listing climbs</h1>

<table>
  <tr>
    <th>Grade</th>
    <th>Session</th>
    <th>Difficulty</th>
    <th>Steepness</th>
    <th>State</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @climbs.each do |climb| %>
  <tr>
    <td><%= climb.grade %></td>
    <td><%= climb.session %></td>
    <td><%= climb.difficulty %></td>
    <td><%= climb.steepness %></td>
    <td><%= climb.state %></td>
    <td><%= link_to 'Show', climb %></td>
    <td><%= link_to 'Edit', edit_climb_path(climb) %></td>
    <td><%= link_to 'Destroy', climb, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Climb', new_climb_path %>
